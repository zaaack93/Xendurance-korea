        "7ac82b1d-1c6f-4574-af69-d3fc200c2957": {
          "type": "item",
          "settings": {
            "title": "What is your return policy?",
            "content": "<p>Xendurance offers a one hundred percent (100%) 30-day, money-back satisfaction guarantee on all Xendurance products.<br\/><br\/>Refunds are valid for 30 days from purchase date. Please email info@xendurance.com if you would like to return or exchange your order.<\/p>"
          }
        },
        "7ec304d7-7dd0-49cd-9c3e-2feb280d7e8a": {
          "type": "item",
          "settings": {
            "title": "Damaged or Faulty Products",
            "content": "<p>In the rare case that you have received a damaged product, please reach out to info@xendurance.com and we will help resolve the issue.<\/p>"
          }
        },
        "c01be31b-62f3-4ff0-a7b1-155f78a05eb5": {
          "type": "item",
          "settings": {
            "title": "How do I initiate a return or exchange?",
            "content": "<p>Please email info@xendurance.com with your order number and we will help get you sorted out!<\/p>"
          }
        }
      },
